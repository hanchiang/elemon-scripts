  "devDependencies": {
    "@types/concurrently": "5.2.1",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "7.2.6",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.170",
    "@types/node": "14.14.25",
    "@types/nodemailer": "6.4.0",
    "@types/request": "2.48.5",
    "@types/request-promise": "4.1.47",
    "@types/shelljs": "0.8.8",
    "@types/web3": "^1.2.2",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "concurrently": "6.0.2",
    "eslint": "7.19.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.7",
    "prettier": "2.4.1",
    "shelljs": "0.8.4",
    "ts-jest": "^27.0.3",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
